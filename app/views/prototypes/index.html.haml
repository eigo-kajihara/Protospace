.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height: "73px")

.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        %li.btn-default.btn-popular
          = link_to "Popular PROTO", "/"
        %li.btn-default.btn-popular
          = link_to "Newest PROTO", "/prototypes/newest"
      = link_to "VIEW TAGS", "/tags", class: "btn btn-default col-lg-1"
.container.proto-list
  .row
    = render partial: 'prototype', collection: @prototypes

.text-center
  = paginate @prototypes
  / %ul.pagination
  /   %li.disabled
  /     %a{"aria-label": "Previous", href: "#"}
  /       %span{"aria-hidden": "true"} «
  /   %li.active
  /     %a{href: "#"}
  /       1
  /       %span.sr-only (current)
  /   %li
  /     %a{href: "#"} 2
  /   %li
  /     %a{href: "#"} 3
  /   %li
  /     %a{href: "#"} 4
  /   %li
  /     %a{href: "#"} 5
  /   %li
  /     %a{"aria-label": "Next", href: "#"}
  /       %span{"aria-hidden": "true"} »



